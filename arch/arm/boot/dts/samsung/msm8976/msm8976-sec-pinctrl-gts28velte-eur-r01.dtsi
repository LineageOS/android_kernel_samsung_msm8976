      cam_front_sensor_stby_active: cam_front_sensor_stby_active{
        drive-strength = <2>; /* 2 MA */
        bias-pull-down; /* PULL DOWN */
        input-enable;
      };
      cam_front_sensor_stby_sleep: cam_front_sensor_stby_sleep{
        drive-strength = <2>; /* 2 MA */
        bias-pull-down; /* PULL DOWN */
        input-enable;
      };
    };

    cam_front_sensor_ctrl_enable {
      qcom,pins = <&gp 63>;
      qcom,num-grp-pins = <1>;
