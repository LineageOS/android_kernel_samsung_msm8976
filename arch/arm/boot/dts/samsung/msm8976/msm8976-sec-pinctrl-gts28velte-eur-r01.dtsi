      qcom,pin-func = <1>;
      label = "cam_mclk0_active";
      cam_mclk0_active: cam_mclk0_active{
        drive-strength = <2>; /* 2 MA */
        bias-disable; /* No PULL */
        output-low;
      };
    };
    cam_mclk0_sleep {
      qcom,pins = <&gp 26>;
      qcom,num-grp-pins = <1>;
      label = "cam_mclk0_sleep";
      cam_mclk0_sleep: cam_mclk0_sleep{
        drive-strength = <2>; /* 2 MA */
        bias-pull-down; /* PULL DOWN */
        output-low;
      };
    };

    cam_mclk1_active {
      qcom,pins = <&gp 27>;
      qcom,num-grp-pins = <1>;
      qcom,pin-func = <1>;
