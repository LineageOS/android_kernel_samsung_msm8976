		charger {
			status = "disable";
			/* Charger */
			battery,charger_name = "sm5705-charger";
			battery,chgen_gpio = <&msm_gpio 132 0x00>;
			battery,chg_float_voltage = <4390>;

			battery,chg_polarity_en = <0>;
			battery,chg_gpio_status = <0>;
