config SAMSUNG_DEBUG_DISPLAY
        bool "Add Samsung Debugging code"
        default n

config FB_MSM_MDSS_SAMSUNG
	depends on FB_MSM_MDSS
	bool "mdss config for samsung product"
	default n
	---help---
	Mdss config signature for samsung product

